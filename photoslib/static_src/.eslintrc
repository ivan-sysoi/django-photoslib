{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "isDev": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "semi": [2, "never"],
    "max-len": [2, {"code": 120}],
    "react/jsx-filename-extension": [2, {"extensions": [".js", ".jsx"]}]
  }
}
